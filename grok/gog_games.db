game=darkd\; dir="$HOME/.local/share/Red Hook Studios/Darkest/profile_0"\
case "$1" in\
  -[0-9]) dir="${dir%0}${1#-}"\; shift \;\;\
esac\
if [ x-r = "x$1" ]\; then\
  sg="$HOME/games/saves/${game}-$2${2:+-}sav.tar.bz2"\
  sdir=`tar tjf "$sg" | head -n 1`\
  sdir=${sdir%/}\
  dir="${dir%/*}/$sdir"\
  echo "Restoring to slot ${sdir#*_}"\
fi\
. /usr/local/share/save-slots.sh\
