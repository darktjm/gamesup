      # Enfuriatingly enough, some are also mixed-case, so all 3 should be present\
      find -name '*[A-Z]*' | while read -r x\; do echo "${x##*/}" | dd conv=lcase 2>/dev/null | while read -r lc\; do test -e "${x%/*}/$lc" || ln -s "${x##*/}" "${x%/*}/$lc"\; done\; done\
